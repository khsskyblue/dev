          "_name": "testimonials2",
          "_customHTML": "<section class=\"features1 vertical-line\" group=\"Features\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"15\" step=\"1\" value=\"0\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"15\" step=\"1\" value=\"10\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Show Card Title\" name=\"showCardTitle\">\n        <input type=\"checkbox\" title=\"Show Text\" name=\"showText\" checked>\n        <input type=\"checkbox\" title=\"Icon\" name=\"showIcons\" checked>\n        <input type=\"checkbox\" title=\"Show Name\" name=\"showName\" checked>\n        <input type=\"checkbox\" title=\"Show Biography\" name=\"showBio\" checked>\n        <header>Color</header>\n        <input type=\"color\" title=\"Icons Color 1\" name=\"IconsColor\" value=\"#f0ad4e\">\n        <input type=\"color\" title=\"Card Background\" value=\"#191919\" name=\"cardBg\">\n        <header>Additional</header>\n        <select title=\"Icons\" name=\"iconsAmount\" condition=\"showIcons\">\n          <option value=\"1\">1</option>\n          <option value=\"2\">2</option>\n          <option value=\"3\">3</option>\n          <option value=\"4\">4</option>\n          <option value=\"5\" selected>5</option>\n          <option value=\"6\">6</option>\n        </select>\n        <select title=\"Cards\" name=\"cardsAmount\">\n          <option value=\"1\">1</option>\n          <option value=\"2\">2</option>\n          <option value=\"3\" selected>3</option>\n          <option value=\"4\">4</option>\n        </select>\n        \n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n          <input type=\"image\" title=\"Background Image\" value=\"../_images/man-3.jpg\" parallax>\n          <input type=\"color\" title=\"Background Color\" value=\"#000000\" selected>\n          <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#000000\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"title col-12 align-center\">\n              <h3 class=\"mbr-section-subtitle mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">TESTIMONIAlS</h3>\n              <h2 class=\"mbr-bold mbr-fonts-style mbr-section-title mbr-white\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">\n                Our Trusting Clients</h2>\n            </div>  \n            <div class=\"card col-12 col-md-6\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n              'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img align-left\" mbr-if=\"showIcons\">\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 1\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 2\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 3\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 4\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 5\"></span>\n                </div>\n                <div class=\"card-box\">\n                  <h4 class=\"card-title align-left mbr-bold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-img\">Photo Sessions</h4>\n                  <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n                      Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer ullamcorper elementum felis in\n                      bibendum. Proin vitae turpis ipsum.</p>\n                  <div class=\"card-people\">\n                    <div class=\"user_image\">\n                      <img src=\"../_images/team/team1.jpg\">\n                    </div>\n                    <div class=\"wrapper-name\">\n                      <div class=\"mbr-author-name mbr-semibold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".mbr-author-name\" mbr-text>\n                        Ben James</div>\n                      <div class=\"mbr-author-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBio\" data-app-selector=\".mbr-author-desc\" mbr-text>\n                        Client</div>\n                    </div>\n                  </div>\n                </div>\n                \n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 1\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img align-left\" mbr-if=\"showIcons\">\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 1\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 2\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 3\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 4\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 5\"></span>\n                </div>\n                <div class=\"card-box\">\n                    <h4 class=\"card-title align-left mbr-bold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-img\">\n                        Occasions\n                    </h4>\n                    <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer ullamcorper elementum felis in\n                        bibendum. Proin vitae turpis ipsum.</p>\n                    <div class=\"card-people\">\n                      <div class=\"user_image\">\n                        <img src=\"../_images/team/team2.jpg\">\n                      </div>\n                      <div class=\"wrapper-name\">\n                        <div class=\"mbr-author-name mbr-semibold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".mbr-author-name\" mbr-text>\n                          John James</div>\n                        <div class=\"mbr-author-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBio\" data-app-selector=\".mbr-author-desc\" mbr-text>\n                          Client</div>\n                      </div>\n                    </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 2\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">\n                <div class=\"card-img align-left\" mbr-if=\"showIcons\">\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 1\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 2\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 3\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 4\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 5\"></span>\n                </div>\n                <div class=\"card-box\">\n                    <h4 class=\"card-title align-left mbr-bold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title, .card-img\">\n                        Coverage\n                    </h4>\n                    <p class=\"mbr-text align-left mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .link\">\n                        Lorem ipsum dolor sit amet, consectetur adipiscing elit. Integer ullamcorper elementum felis in\n                        bibendum. Proin vitae turpis ipsum.</p>\n                    <div class=\"card-people\">\n                      <div class=\"user_image\">\n                        <img src=\"../_images/team/team4.jpg\">\n                      </div>\n                      <div class=\"wrapper-name\">\n                        <div class=\"mbr-author-name mbr-semibold mbr-fonts-style mbr-white\" mbr-theme-style=\"display-7\" mbr-if=\"showName\" data-app-selector=\".mbr-author-name\" mbr-text>\n                          Ben James</div>\n                        <div class=\"mbr-author-desc mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showBio\" data-app-selector=\".mbr-author-desc\" mbr-text>\n                          Client</div>\n                      </div>\n                    </div>\n                </div>\n              </div>\n            </div>\n            <div class=\"card col-12 col-md-6\" mbr-if=\"cardsAmount > 3\" mbr-class=\"{'col-lg-3': cardsAmount == '4',\n                             'col-lg-4': cardsAmount == '3'}\">\n              <div class=\"card-inner\">                \n                <div class=\"card-img align-left\" mbr-if=\"showIcons\">\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 1\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 2\"></span>\n                  <span mbr-icon class=\"mbr-iconfont mbr-iconfont-1 mobi-mbri mobi-mbri-star\" mbr-if=\"iconsAmount > 3\"