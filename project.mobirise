          "_name": "design-block",
          "_customHTML": "<section mbr-id=\"_anchor\" class=\"mbr-section article content1\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"8\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"8\" step=\"1\" value=\"3\">\n        <input type=\"color\" title=\"Background Color\" name=\"bgColor\" value=\"#ffffff\">\n      <input type=\"checkbox\" title=\"Show Social Options\" name=\"Socialsignup\" checked>\n      <input type=\"checkbox\" title=\"Show logo\" name=\"logo\" checked>\n      <select title=\"Form Type\" name=\"ftype\">\n            <option value=\"1\">Signup</option>\n            <option value=\"2\">Login</option>\n            <option value=\"3\" selected>Forget password</option>\n        </select>\n      <hr><header>Form input settings</header>\n      <input type=\"checkbox\" title=\"Show Firstname\" name=\"firstname\" checked>\n      <input type=\"checkbox\" title=\"Show Lastname\" name=\"lastname\" checked>\n      <input type=\"checkbox\" title=\"Show Email\" name=\"email\" checked>\n      <input type=\"checkbox\" title=\"Show Joblist\" name=\"joblist\" checked>\n      <input type=\"checkbox\" title=\"Show Phone\" name=\"phone\" checked>\n      <input type=\"checkbox\" title=\"Show Password\" name=\"password\" checked>\n      <input type=\"checkbox\" title=\"Show Confirm Pass\" name=\"cpassword\" checked>\n    <!-- End block parameters -->\n    </mbr-parameters> \n\n    <!-- Navbar-->\n<header class=\"header\" mbr-if=\"logo\">\n    <nav class=\"navbar navbar-expand-lg navbar-light py-3\">\n        <div class=\"container\">\n            <!-- Navbar Brand -->\n            <a href=\"#\" class=\"navbar-brand\">\n                <img src=\"@PROJECT_PATH@/assets/images/505px-apple-logo-black.svg-128x152.png\" alt=\"logo\" width=\"150\" title>\n            </a>\n        </div>\n    </nav>\n</header>\n\n\n<div class=\"container\" mbr-form>\n    <div class=\"row py-5 mt-4 align-items-center\">\n        <!-- For Demo Purpose -->\n        <div class=\"col-md-5 pr-lg-5 mb-5 mb-md-0\">\n            <img src=\"@PROJECT_PATH@/assets/images/271085593075211-128x118-1.png\" alt class=\"img-fluid mb-3 d-none d-md-block\" title>\n            <h2>Signup, Login and Reset password form</h2>\n            <p class=\"font-italic text-muted mb-0\">This is a three in one mobirise design block, i:e signup, login and password reset form.<br><br></p>\n            <p class=\"font-italic text-muted\">Contact me on WhatsApp <a href=\"https://wa.me/+2349066849761\">+2349066849761</a> for your costume design block with it server side code.</p>\n        </div>\n      \n        <!-- Registeration Form -->\n        <div class=\"col-md-7 col-lg-6 ml-auto\" mbr-if=\"ftype == 1\">\n            <form action=\"youremailaddress2@gmail.com\" name=\"Mobirise Form\" method=\"POST\" class=\"mbr-form\">\n              <div class=\"row\">\n                        <div hidden=\"hidden\" data-form-alert class=\"alert alert-success col-12\">Thanks for submitting!</div>\n                        <div hidden=\"hidden\" data-form-alert-danger class=\"alert alert-danger col-12\">\n                        </div>\n                    </div>\n                <div class=\"row\">\n\n                    <!-- First Name -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"firstname\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"firstName\" type=\"text\" name=\"firstname\" placeholder=\"First Name\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Firstname\" required=\"required\">\n                    </div>\n\n                    <!-- Last Name -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"lastname\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-user text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"lastName\" type=\"text\" name=\"lastname\" placeholder=\"Last Name\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Lastname\" required=\"required\">\n                    </div>\n\n                    <!-- Email Address -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"email\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-envelope text-muted\"></i>\n                            </span>\n                        </div>\n                        <input id=\"email\" type=\"email\" name=\"email\" placeholder=\"Email Address\" class=\"form-control bg-white border-left-0 border-md\" data-form-field=\"Email\" required=\"required\">\n                    </div>\n\n                    <!-- Phone Number -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"phone\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-phone-square text-muted\"></i>\n                            </span>\n                        </div>\n                        <select id=\"countryCode\" name=\"countryCode\" style=\"max-width: 80px\" class=\"custom-select form-control bg-white border-left-0 border-md h-100 font-weight-bold text-muted\">\n                            <option value>+12</option>\n                            <option value>+10</option>\n                            <option value>+15</option>\n                            <option value>+18</option>\n                        </select>\n                        <input id=\"phoneNumber\" type=\"tel\" name=\"phone\" placeholder=\"Phone Number\" class=\"form-control bg-white border-md border-left-0 pl-3\" data-form-field=\"Phone\" required=\"required\">\n                    </div>.\n\n\n                    <!-- Job -->\n                    <div class=\"input-group col-lg-12 mb-4\" mbr-if=\"joblist\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-black-tie text-muted fa-briefcase fas\"></i>\n                            </span>\n                        </div>\n                        <select id=\"job\" name=\"jobtitle\" class=\"form-control custom-select bg-white border-left-0 border-md\" data-form-field=\"Joblist\" required=\"required\">\n                            <option value>Choose your job</option>\n                            <option value>Designer</option>\n                            <option value>Developer</option>\n                            <option value>Manager</option>\n                            <option value>Accountant</option>\n                        </select>\n                    </div>\n\n                    <!-- Password -->\n                    <div class=\"input-group col-lg-6 mb-4\" mbr-if=\"password\">\n                        <div class=\"input-group-prepend\">\n                            <span class=\"input-group-text bg-white px-4 border-md border-right-0\">\n                                <i mbr-icon class=\"fa fa-lock text-muted\"></i>\n                    